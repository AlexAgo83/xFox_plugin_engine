package com.aura.engine.inputMap.key;

import com.aura.engine.inputMap.EPMapCE;
import com.aura.engine.utils.Orientation;

public class EPKeyMapCEMovement extends EPMapCE {
	private final Orientation orientation;
	
	public EPKeyMapCEMovement(int id, String label, Orientation orientation, int configId) {
		super(id, label, configId);
		this.orientation = orientation;
	}
	
	public Orientation getOrientation() {
		return orientation;
	}
}
