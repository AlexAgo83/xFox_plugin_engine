package com.aura.engine.event;

import com.aura.base.event.AbstractEvent;

public abstract class EPEventInput extends AbstractEvent {
	private int code;
	
	public EPEventInput(int id) {
		super(id);
		setSilent(true);
	}
	
	public int getCode() {
		return code;
	}
	public void setCode(int code) {
		this.code = code;
	}
}